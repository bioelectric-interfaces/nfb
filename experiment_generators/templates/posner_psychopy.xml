			<iDelayMs>0</iDelayMs>
			<bSmoothingEnabled>0</bSmoothingEnabled>
			<dSmoothingWindow>100</dSmoothingWindow>
		</DerivedSignal>
		<CompositeSignal>
			<sSignalName>{{composite_signal}}</sSignalName>
			<sExpression>(left-right)/(left+right)</sExpression>
			<bSmoothingEnabled>{{enable_smoothing}}</bSmoothingEnabled>
			<dSmoothingWindow>{{smooth_window}}</dSmoothingWindow>
		</CompositeSignal>
		<CompositeSignal>
			<sSignalName>EYE_TRACK</sSignalName>
			<sExpression>(left_eye-right_eye)</sExpression>
			<bSmoothingEnabled>1</bSmoothingEnabled>
			<dSmoothingWindow>100</dSmoothingWindow>
		</CompositeSignal>
