<?xml version="1.0" encoding="utf-8"?>
<NeurofeedbackSignalSpecs>
	<bDC>0</bDC>
	<dBCThresholdAdd>{{baseline_cor_threshold}}</dBCThresholdAdd>
	<bUseBCThreshold>{{ use_baseline_correction }}</bUseBCThreshold>
	<sPrefilterBand>None None</sPrefilterBand>
	<sExperimentName>{{ experiment }}</sExperimentName>
	<sInletType>lsl</sInletType>
	<sStreamName>{{ stream_name }}</sStreamName>
	<sEventsStreamName></sEventsStreamName>
	<sRawDataFilePath></sRawDataFilePath>
	<sFTHostnamePort>localhost:1972</sFTHostnamePort>
	<bPlotRaw>1</bPlotRaw>
	<bPlotSignals>1</bPlotSignals>
	<bPlotSourceSpace>0</bPlotSourceSpace>
	<bShowSubjectWindow>1</bShowSubjectWindow>
	<fRewardPeriodS>0.05</fRewardPeriodS>
	<sReference></sReference>
	<sReferenceSub></sReferenceSub>
	<bUseExpyriment>0</bUseExpyriment>
	<bShowPhotoRectangle>0</bShowPhotoRectangle>
	<sVizNotchFilters>0</sVizNotchFilters>
	<vSignals>
		<DerivedSignal>
			<sSignalName>Alpha_Left</sSignalName>
			<SpatialFilterMatrix>{{left_spatial_filter_scalp}}</SpatialFilterMatrix>
			<bDisableSpectrumEvaluation>0</bDisableSpectrumEvaluation>
			<fSmoothingFactor>0.7</fSmoothingFactor>
			<fFFTWindowSize>{{fft_window}}</fFFTWindowSize>
			<fBandpassLowHz>{{ band_low }}</fBandpassLowHz>
			<fBandpassHighHz>{{ band_high }}</fBandpassHighHz>
			<fAverage></fAverage>
			<fStdDev></fStdDev>
			<bBCIMode>0</bBCIMode>
			{% if source_roi_left -%}
			{% for roi in source_roi_left %}
			<lROILabel>{{roi}}</lROILabel>
			{% endfor %}
			{% endif %}
			<bNFBType>{{source_fb}}</bNFBType>
			<sTemporalType>envdetector</sTemporalType>
			<sTemporalFilterType>{{ temp_filt_type }}</sTemporalFilterType>
			<fTemporalFilterButterOrder>2</fTemporalFilterButterOrder>
			<sTemporalSmootherType>exp</sTemporalSmootherType>
			<iDelayMs>0</iDelayMs>
		</DerivedSignal>
		<DerivedSignal>
			<sSignalName>Alpha_Right</sSignalName>
			<SpatialFilterMatrix>{{right_spatial_filter_scalp}}</SpatialFilterMatrix>
			<bDisableSpectrumEvaluation>0</bDisableSpectrumEvaluation>
			<fSmoothingFactor>0.7</fSmoothingFactor>
			<fFFTWindowSize>{{fft_window}}</fFFTWindowSize>
			<fBandpassLowHz>{{ band_low }}</fBandpassLowHz>
			<fBandpassHighHz>{{ band_high }}</fBandpassHighHz>
			<fAverage></fAverage>
			<fStdDev></fStdDev>
			<bBCIMode>0</bBCIMode>
			{% for roi in source_roi_right %}
			<lROILabel>{{roi}}</lROILabel>
			{% endfor %}
			<bNFBType>{{source_fb}}</bNFBType>
			<sTemporalType>envdetector</sTemporalType>
			<sTemporalFilterType>{{ temp_filt_type }}</sTemporalFilterType>
			<fTemporalFilterButterOrder>2</fTemporalFilterButterOrder>
			<sTemporalSmootherType>exp</sTemporalSmootherType>
			<iDelayMs>0</iDelayMs>
		</DerivedSignal>
		<CompositeSignal>
			<sSignalName>{{composite_signal}}</sSignalName>
			<sExpression>(Alpha_Left-Alpha_Right)/(Alpha_Left+Alpha_Right)</sExpression>
			<bSmoothingEnabled>{{enable_smoothing}}</bSmoothingEnabled>
			<dSmoothingWindow>{{smooth_window}}</dSmoothingWindow>
		</CompositeSignal>
	</vSignals>
