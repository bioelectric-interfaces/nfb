<?xml version="1.0" encoding="utf-8"?>
<NeurofeedbackSignalSpecs>
	<bDC>0</bDC>
	<sPrefilterBand>None None</sPrefilterBand>
	<sExperimentName>{{ experiment }}</sExperimentName>
	<sInletType>lsl</sInletType>
	<sStreamName>{{ stream_name }}</sStreamName>
	<sEventsStreamName></sEventsStreamName>
	<sRawDataFilePath></sRawDataFilePath>
	<sFTHostnamePort>localhost:1972</sFTHostnamePort>
	<bPlotRaw>1</bPlotRaw>
	<bPlotSignals>1</bPlotSignals>
	<bPlotSourceSpace>0</bPlotSourceSpace>
	<bShowSubjectWindow>1</bShowSubjectWindow>
	<fRewardPeriodS>0.25</fRewardPeriodS>
	<sReference></sReference>
	<sReferenceSub></sReferenceSub>
	<bUseExpyriment>0</bUseExpyriment>
	<bShowPhotoRectangle>0</bShowPhotoRectangle>
	<sVizNotchFilters>0</sVizNotchFilters>
	<vSignals>
		<DerivedSignal>
			<sSignalName>Alpha_Left</sSignalName>
			<SpatialFilterMatrix>{{left_spatial_filter_scalp}}</SpatialFilterMatrix>
			<bDisableSpectrumEvaluation>0</bDisableSpectrumEvaluation>
			<fSmoothingFactor>0.3</fSmoothingFactor>
			<fFFTWindowSize>1000</fFFTWindowSize>
			<fBandpassLowHz>{{ band_low }}</fBandpassLowHz>
			<fBandpassHighHz>{{ band_high }}</fBandpassHighHz>
			<fAverage></fAverage>
			<fStdDev></fStdDev>
			<bBCIMode>0</bBCIMode>
			{% for roi in source_roi_left %}
			<lROILabel>{{roi}}</lROILabel>
			{% endfor %}
			<bNFBType>{{source_fb}}</bNFBType>
			<sTemporalType>envdetector</sTemporalType>
			<sTemporalFilterType>{{ temp_filt_type }}</sTemporalFilterType>
			<fTemporalFilterButterOrder>2</fTemporalFilterButterOrder>
			<sTemporalSmootherType>savgol</sTemporalSmootherType>
			<iDelayMs>0</iDelayMs>
		</DerivedSignal>
		<DerivedSignal>
			<sSignalName>Alpha_Right</sSignalName>
			<SpatialFilterMatrix>{{right_spatial_filter_scalp}}</SpatialFilterMatrix>
			<bDisableSpectrumEvaluation>0</bDisableSpectrumEvaluation>
			<fSmoothingFactor>0.3</fSmoothingFactor>
			<fFFTWindowSize>1000</fFFTWindowSize>
			<fBandpassLowHz>{{ band_low }}</fBandpassLowHz>
			<fBandpassHighHz>{{ band_high }}</fBandpassHighHz>
			<fAverage></fAverage>
			<fStdDev></fStdDev>
			<bBCIMode>0</bBCIMode>
			{% for roi in source_roi_right %}
			<lROILabel>{{roi}}</lROILabel>
			{% endfor %}
			<bNFBType>{{source_fb}}</bNFBType>
			<sTemporalType>envdetector</sTemporalType>
			<sTemporalFilterType>{{ temp_filt_type }}</sTemporalFilterType>
			<fTemporalFilterButterOrder>2</fTemporalFilterButterOrder>
			<sTemporalSmootherType>savgol</sTemporalSmootherType>
			<iDelayMs>0</iDelayMs>
		</DerivedSignal>
		<CompositeSignal>
			<sSignalName>{{composite_signal}}</sSignalName>
			<sExpression>(Alpha_Right-Alpha_Left)/(Alpha_Right+Alpha_Left)</sExpression>
		</CompositeSignal>
	</vSignals>
	<vProtocols>
		<FeedbackProtocol>
			<sProtocolName>Start</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>0.1</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>ExperimentStart</sFb_type>
			<cString>We will now record your baseline EEG for 2 minutes.
&lt;br&gt;Please completely relax with your eyes open.
&lt;br&gt;&lt;br&gt;Press SPACE to start.</cString>
			<bVoiceover>0</bVoiceover>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>0.0</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>0</iRandomBound>
			<sVideoPath></sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>1.0</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
		</FeedbackProtocol>
		<FeedbackProtocol>
			<sProtocolName>baseline</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>{{baseline_duration}}</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>Baseline</sFb_type>
			<cString></cString>
			<bVoiceover>0</bVoiceover>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>-1.0</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>0</iRandomBound>
			<sVideoPath></sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>1.0</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
		</FeedbackProtocol>
		<FeedbackProtocol>
			<sProtocolName>NFB</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>5.0</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>Feedback</sFb_type>
			<cString></cString>
			<bVoiceover>0</bVoiceover>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>0.01</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>3</iRandomBound>
			<sVideoPath></sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>0.95</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
			<tFixationCrossColour>Black</tFixationCrossColour>
		</FeedbackProtocol>
		<FeedbackProtocol>
			<sProtocolName>Input</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>0.1</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>ParticipantChoice</sFb_type>
			<cString></cString>
			<bVoiceover>0</bVoiceover>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>0.0</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>0</iRandomBound>
			<sVideoPath></sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>1.0</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
		</FeedbackProtocol>
		<FeedbackProtocol>
			<sProtocolName>delay</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>3.0</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>FixationCross</sFb_type>
			<cString>?</cString>
			<bVoiceover>0</bVoiceover>
			<bProbe>1</bProbe>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>0.0</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>0</iRandomBound>
			<sVideoPath>../defaults/lena.png</sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>1.0</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
			<tFixationCrossColour>None</tFixationCrossColour>
		</FeedbackProtocol>
        <FeedbackProtocol>
			<sProtocolName>End</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>0.1</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>ExperimentStart</sFb_type>
			<cString>Experiment finished. </cString>
			<bVoiceover>0</bVoiceover>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>-1.0</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>0</iRandomBound>
			<sVideoPath>../defaults/lena.png</sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>1.0</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
			<tFixationCrossColour>Black</tFixationCrossColour>
		</FeedbackProtocol>
		<FeedbackProtocol>
			<sProtocolName>EyeCalib</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>12.0</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>EyeCalibration</sFb_type>
			<cString></cString>
			<bVoiceover>0</bVoiceover>
			<bProbe>0</bProbe>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>0.0</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>0</iRandomBound>
			<sVideoPath>../defaults/lena.png</sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>1.0</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
			<tFixationCrossColour>Black</tFixationCrossColour>
		</FeedbackProtocol>
		<FeedbackProtocol>
			<sProtocolName>EyeCalib_Start</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>0.1</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>ExperimentStart</sFb_type>
			<cString>Fixate on the dot or the cross.
&lt;br&gt;Press SPACE to start.</cString>
			<bVoiceover>0</bVoiceover>
			<bProbe>0</bProbe>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>0.0</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>0</iRandomBound>
			<sVideoPath>../defaults/lena.png</sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>1.0</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
			<tFixationCrossColour>Black</tFixationCrossColour>
		</FeedbackProtocol>
		<FeedbackProtocol>
			<sProtocolName>fc_w</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>4.0</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>FixationCross</sFb_type>
			<cString></cString>
			<bVoiceover>0</bVoiceover>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>0.0</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>0</iRandomBound>
			<sVideoPath></sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>1.0</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
			<tFixationCrossColour>White</tFixationCrossColour>
		</FeedbackProtocol>
		<FeedbackProtocol>
			<sProtocolName>fc_b</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>1.0</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>FixationCross</sFb_type>
			<cString></cString>
			<bVoiceover>0</bVoiceover>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>0.0</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>0</iRandomBound>
			<sVideoPath></sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>1.0</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
			<tFixationCrossColour>Black</tFixationCrossColour>
		</FeedbackProtocol>
		<FeedbackProtocol>
			<sProtocolName>NFB_Start</sProtocolName>
			<bUpdateStatistics>0</bUpdateStatistics>
			<sStatisticsType>meanstd</sStatisticsType>
			<iDropOutliers>0</iDropOutliers>
			<bSSDInTheEnd>0</bSSDInTheEnd>
			<fDuration>0.1</fDuration>
			<fRandomOverTime>0.0</fRandomOverTime>
			<fbSource>{{composite_signal}}</fbSource>
			<sFb_type>ExperimentStart</sFb_type>
			<cString>You will now begin NFB Training.
&lt;br&gt;Keep your eyes fixated on the centre cross.
&lt;br&gt;Try to guess if the 2nd image is correct by making the 1st image visable.
&lt;br&gt;Your score shows how long you could keep the image visible.
&lt;br&gt;Try to get a higher score each time.
&lt;br&gt;&lt;br&gt;
Press SPACE to start.</cString>
			<bVoiceover>0</bVoiceover>
			<bProbe>0</bProbe>
			<bUseExtraMessage>0</bUseExtraMessage>
			<cString2></cString2>
			<fBlinkDurationMs>50</fBlinkDurationMs>
			<fBlinkThreshold>0.0</fBlinkThreshold>
			<sMockSignalFilePath></sMockSignalFilePath>
			<sMockSignalFileDataset>protocol1</sMockSignalFileDataset>
			<iMockPrevious>0</iMockPrevious>
			<bReverseMockPrevious>0</bReverseMockPrevious>
			<bRandomMockPrevious>0</bRandomMockPrevious>
			<sRewardSignal>{{composite_signal}}</sRewardSignal>
			<bRewardThreshold>0.0</bRewardThreshold>
			<bShowReward>0</bShowReward>
			<bPauseAfter>0</bPauseAfter>
			<bBeepAfter>0</bBeepAfter>
			<iRandomBound>0</iRandomBound>
			<sVideoPath>../defaults/lena.png</sVideoPath>
			<sMSignal>None</sMSignal>
			<fMSignalThreshold>1.0</fMSignalThreshold>
			<bMockSource>0</bMockSource>
			<bEnableDetectionTask>0</bEnableDetectionTask>
			<bAutoBCIFit>0</bAutoBCIFit>
			<tFixationCrossColour>Black</tFixationCrossColour>
		</FeedbackProtocol>
	</vProtocols>
	<vPGroups>
		<PGroup>
			<sName>fb_group</sName>
			<sList>fc_w fc_b NFB delay Input</sList>
			<sNumberList>1 1 1 1 1</sNumberList>
			<bShuffle>0</bShuffle>
			<sSplitBy></sSplitBy>
		</PGroup>
	</vPGroups>
	<vPSequence>
		<s>EyeCalib_Start</s>
		<s>EyeCalib</s>
		<s>Start</s>
		<s>baseline</s>
		<s>NFB_Start</s>
        {% for i in range(0, number_nfb_tasks) %}{{ i }}
		<s>fb_group</s>
        {% endfor %}
        <s>End</s>
	</vPSequence>
</NeurofeedbackSignalSpecs>